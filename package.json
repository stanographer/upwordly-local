{
  "name": "upwordly-frontend",
  "version": "1.0.0",
  "description": "Front-end for Upword.ly.",
  "main": "index.js",
  "scripts": {
    "env": "scp ~/Developer/upwordly-local/.env stanley@upword.ly:~/upwordly-local",
    "build": "NODE_ENV=production next build",
    "buildlocal": "NODE_ENV=local next build",
    "dev": "next",
    "lint": "DEBUG=eslint:cli-engine eslint --fix . && echo 'Lint complete.'",
    "pm2dev": "pm2 start npm --name \"next\" -- run dev",
    "pm2prod": "npm run build && pm2 start npm --name \"next\" -- start",
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "start": "NODE_ENV=production next start",
    "startlocal": "NODE_ENV=local next start"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stanographer/upwordly-local.git"
  },
  "keywords": [
    "steno",
    "stenography",
    "live",
    "transcription",
    "captioning",
    "captions",
    "transcript"
  ],
  "author": "Stanley Sakai <stanley@stanographer.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stanographer/upwordly-local/issues"
  },
  "homepage": "https://upword.ly",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.21",
    "@fortawesome/free-solid-svg-icons": "^5.10.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@fullpage/react-fullpage": "^0.1.15",
    "@teamwork/sharedb": "^3.1.0",
    "@zeit/next-css": "^1.0.1",
    "check-prop-types": "^1.1.2",
    "dotenv": "^8.0.0",
    "firebase": "^6.3.4",
    "isomorphic-ws": "^4.0.1",
    "jumprope": "^1.2.1",
    "next": "^9.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ot-text": "^1.0.2",
    "react": "^16.8.6",
    "react-color": "^2.17.3",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.6",
    "react-geosuggest": "^2.12.0",
    "react-modal": "^3.9.1",
    "react-scroll": "^1.7.12",
    "react-spinners-kit": "^1.9.0",
    "react-toastify": "^5.3.2",
    "react-typed": "^1.1.2",
    "reconnecting-websocket": "^4.1.10",
    "styled-components": "^4.3.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "uuid": "^3.3.2",
    "webpack": "^4.39.1",
    "ws": "^7.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.1.0",
    "postcss-easy-import": "^3.0.0",
    "prop-types": "^15.7.2",
    "require": "^2.4.20",
    "tailwindcss": "^1.0.6",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
