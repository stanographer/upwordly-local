{
  "name": "upwordly-local",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@teamwork/sharedb": "^3.1.0",
    "bootstrap": "^4.3.1",
    "ot-text": "^1.0.2",
    "public-ip": "^3.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intersection-observer": "^8.23.0",
    "react-intersection-visible": "^2.1.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "react-scroll": "^1.7.11",
    "reactstrap": "^8.0.0",
    "reconnecting-websocket": "^4.1.10",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar ./src -o ./build",
    "start": "react-scripts start",
    "startdev": "NODE_ENV=development react-scripts start && node-sass-chokidar ./src -o ./build --watch",
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "Displays live transcriptions from Upwordly locally.",
  "main": "App.js",
  "devDependencies": {
    "custom-env": "^1.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "node-sass-chokidar": "^1.3.4",
    "prop-types": "^15.7.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "setupFiles": [
    "./src/setupTests.js"
  ],
  "keywords": [
    "steno",
    "captioning",
    "realtime",
    "captioning",
    "realtime",
    "live",
    "transcription",
    "stenographer",
    "stenography",
    "repository",
    "transcript",
    "repository"
  ],
  "author": "Stanley Sakai",
  "license": "GPL-3.0-or-later",
  "homepage": "https://upword.ly"
}
